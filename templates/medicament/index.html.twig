{# templates/medicament/index.html.twig #}{% extends 'base.html.twig' %}{% block title %}Medicament List{% endblock %}{% block body %}    <div class="container mt-5">        <h1 class="mb-4">Medicament List</h1>        <table class="table table-striped table-bordered">            <thead class="thead-dark">                <tr>                    <th>ID</th>                    <th>Nom</th>                    <th>Prix</th>                    <th>Qte Stock</th>                    <th>Sur Ordonnance</th>                    <th>Type Med</th>                    <th>Actions</th>                </tr>            </thead>            <tbody>                {% for medicament in medicaments %}                    <tr>                        <td>{{ medicament.id }}</td>                        <td>{{ medicament.nom }}</td>                        <td>{{ medicament.prix }}</td>                        <td>{{ medicament.qteStock }}</td>                        <td>{{ medicament.surOrdonnance ? 'Yes' : 'No' }}</td>                        <td>{{ medicament.TypeMed }}</td>                        <td>                            <a href="{{ path('medicament_edit', {'id': medicament.id}) }}" class="btn btn-warning btn-sm">Edit</a>                            <form method="post" action="{{ path('medicament_delete', {'id': medicament.id}) }}" style="display:inline-block;" onsubmit="return confirm('Are you sure you want to delete this item?');">                                <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ medicament.id) }}">                                <button class="btn btn-danger btn-sm" type="submit">Delete</button>                            </form>                        </td>                    </tr>                {% endfor %}            </tbody>        </table>        <a href="{{ path('medicament_new') }}" class="btn btn-success mt-3">Create new Medicament</a>    </div>{% endblock %}